{
  "name": "N/Word",
  "version": "0.0.1",
  "description": "Masks a common racial epithet from web pages",
  "manifest_version": 2,

  "content_scripts": [
    {
      "matches": ["*://*/*", "file://*"],
      "all_frames": true,
      "js": ["nword.js"],
      "run_at": "document_idle"
    }
  ]
}
